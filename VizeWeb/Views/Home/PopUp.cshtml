@{
    string PopupBaslik = "Takim Olustur";
    string partialIsım = "TakimOlustur";

}
<div class="modal fade bd-example-modal-lg" id="exampleModal" tabindex="-1" role="dialog"
     aria-labelledby="exampleModalLabel" aria-hidden="true">
    <div class="modal-dialog modal-lg" role="document">
        <div class="modal-content">
            <div class="modal-header">
                <h5 class="modal-title" id="exampleModalLabel">@PopupBaslik</h5>
            </div>
            <div class="modal-body" id="modalicerik">
                 @Html.Partial(partialIsım, null);
            </div>
            <div class="modal-footer">
                <button type="button" id="AyarlarıKaydet" class="btn btn-primary">@PopupBaslik</button>
            </div>
        </div>
    </div>
</div>